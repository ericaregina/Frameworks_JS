{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "meu-app-angular": {
      "projectType": "application",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "options": {
            "outputPath": "dist/meu-app-angular",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json"
          }
        },
        "serve": {
          "options": {
            "browserTarget": "meu-app-angular:build"
          }
        }
      }
    }
  },
  "defaultProject": "meu-app-angular"
}
